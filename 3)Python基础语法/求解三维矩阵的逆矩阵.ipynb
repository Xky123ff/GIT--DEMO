   "source": [
    "# 求解三维矩阵的逆矩阵"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "d3ae37d6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "逆矩阵： [[ 1.  -0.5  0. ]\n",
      " [ 0.   0.5  0. ]\n",
      " [ 1.  -2.   1. ]]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "# 定义3x3矩阵\n",
    "A = np.array([[1,   1, 0],\n",
    "              [0, 2, 0],\n",
    "              [-1, 3, 1]])\n",
    "\n",
    "# 计算逆矩阵\n",
    "A_inv = np.linalg.inv(A)\n",
    "print(\"逆矩阵：\", A_inv)\n"
   ]
  }
